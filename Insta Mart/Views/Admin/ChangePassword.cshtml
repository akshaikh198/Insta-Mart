@{
    ViewBag.Title = "ChangePassword";
    Layout = "~/Views/Shared/AdminMaster.cshtml";
}

<main class="app-content">
      <div class="app-title">
        <div>
          <h1><i class="fa fa-edit"></i> Form Samples</h1>
          <p>Sample forms</p>
        </div>
        <ul class="app-breadcrumb breadcrumb">
          <li class="breadcrumb-item"><i class="fa fa-home fa-lg"></i></li>
          <li class="breadcrumb-item">Forms</li>
          <li class="breadcrumb-item"><a href="#">Sample Forms</a></li>
        </ul>
      </div>
      <div class="row">

		
	
        <div class="col-md-6">
		
          <div class="tile">
            <h3 class="tile-title">Change Password</h3>
            <div class="tile-body">
              <form class="form-horizontal" action="/Admin/ChangePassword" method="post">
                <div class="form-group row">
                  <label class="control-label col-md-3">Old Pass</label>
                  <div class="col-md-8">
                    <input class="form-control" type="text" placeholder="Old Password" name="oldpass">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="control-label col-md-3">New Pass</label>
                  <div class="col-md-8">
                    <input class="form-control col-md-8" type="text" placeholder="New Password" name="newpass">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="control-label col-md-3">Confirm Pass</label>
                  <div class="col-md-8">
                    <input class="form-control col-md-8" type="text" placeholder="Confirm Password" name="confirmpass">
                  </div>
                </div>
              
               
                
              
            </div>
            <div class="tile-footer">
              <div class="row">
                <div class="col-md-8 col-md-offset-3">
                  <button class="btn btn-success" type="submit"><i class="fa fa-fw fa-lg fa-check-circle"></i>Change</button>&nbsp;&nbsp;&nbsp;
                    
                    <button class="btn btn-Danger" type="reset" value="reset"><i class="fa fa-fw fa-lg fa-times-circle"></i>Reset</button>
                </div>
              </div>
            </div>
              </form>
          </div>
        </div>
        
		
      </div>
    </main>







